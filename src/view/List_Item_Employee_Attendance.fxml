<HBox fx:id="list_item_emp" alignment="CENTER_LEFT" prefWidth="403.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
    </padding>
    <children>
        <Circle fx:id="circle" fill="WHITE" radius="25.0" stroke="WHITE" strokeType="INSIDE" />
      <VBox alignment="CENTER_LEFT">
         <children>
            <Label fx:id="title" text="Gilbert John A. Cuaresma">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label fx:id="subtitle" text="Sched: 10:00 AM | TIme In: 11:00 AM" />
         </children>
      </VBox>
      <Region HBox.hgrow="ALWAYS" />
      <Label fx:id="statusL" text="LATE" />
    </children>
</HBox>
